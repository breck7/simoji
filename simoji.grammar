  compile() {
    const {rows, cols, positionSet} = this.getParent()
    let objects = parseInt(this.getWord(1))
     let startBoard = []
     let char = this.getWord(2)
     while (objects) {
       startBoard.push(`${char} ${yodash.getRandomLocation(rows, cols, positionSet)}`)
       objects--
     }
     return startBoard.join("\n")
  }
