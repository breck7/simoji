agentDefinitionNode
 inScope commentNode abstractEventNode abstractAgentAttributeNode abstractBehaviorAttributeNode
 cells keywordCell
 catchAllNodeType errorNode
 compiler
  stringTemplate 
 javascript
  compile() {
    const root = this.getRootNode()
    const name = root.agentKeywordMap[this.getWord(0)]
    const normal = super.compile()
    const behaviors = this.filter(node => node.getNodeTypeId() === "abstractBehaviorAttributeNode").map(behavior => `"${behavior.getLine()}"`).join(",")
    return `class ${name} extends Agent {
      icon = "${this.getWord(0)}"
      behaviors = [${behaviors}]
      ${normal}
    }`
  }
